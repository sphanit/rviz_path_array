         base_class_type="rviz::Display">
    <description>
      Displays nav_msgs/Path messages.
    </description>
    <message_type>nav_msgs/Path</message_type>
  </class> -->
  <class name="rviz_plugin_tutorials/HumanPathArray"
         type="rviz_plugin_tutorials::HumanPathArrayDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays hanp_msgs/HumanPathArray messages.
    </description>
    <message_type>hanp_msgs/HumanPathArray</message_type>
  </class>
  <class name="rviz_plugin_tutorials/PlantFlag"
         type="rviz_plugin_tutorials::PlantFlagTool"
         base_class_type="rviz::Tool">
    <description>
      Tool for planting flags on the ground plane in rviz.
    </description>
  </class>
</library>
